{
    "extends": [
        "config:base"
    ],
    "assigneesFromCodeOwners": true,
        "packageRules": [
        {
            "matchPackagePatterns": ["^Microsoft.*", "^System.*"],
            "groupName": "all microsoft dependencies",
            "groupSlug": "all-microsoft",
            "labels": ["dependencies"]
        },
        {
            "matchPackagePatterns": ["^AspNetCore.HealthChecks.*"],
            "groupName": "all AspNetCore HealthChecks dependencies",
            "groupSlug": "all-aspnetcore-healthchecks",
            "labels": ["dependencies"]
        },
        {
            "matchPackagePatterns": ["^Audit.*"],
            "groupName": "all audit.net dependencies",
            "groupSlug": "all-audit-net",
            "labels": ["dependencies"]
        },
        {
            "matchPackagePatterns": ["^Exceptionless.*"],
            "groupName": "all exceptionless dependencies",
            "groupSlug": "all-exceptionless",
            "labels": ["dependencies"]
        },
        {
            "matchPackagePatterns": ["^Foundatio.*"],
            "groupName": "all foundatio dependencies",
            "groupSlug": "all-foundatio",
            "labels": ["dependencies"]
        },
        {
            "matchPackagePatterns": ["^MediatR","^MediatR.*"],
            "groupName": "all mediatr dependencies",
            "groupSlug": "all-mediatr",
            "labels": ["dependencies"]
        },
        {
            "matchPackagePatterns": ["^Microsoft.Fast.*"],
            "groupName": "all microsoft FAST dependencies",
            "groupSlug": "all-microsoft-fast",
            "labels": ["dependencies"]
        },
        {
            "matchPackagePatterns": ["^Microsoft.Identity.*"],
            "groupName": "all microsoft identity dependencies",
            "groupSlug": "all-microsoft-identity",
            "labels": ["dependencies"]
        },
        {
            "matchPackagePatterns": ["^Microsoft.NET.Test.*"],
            "groupName": "all microsoft net test dependencies",
            "groupSlug": "all-microsoft-net-test",
            "labels": ["dependencies"]
        },
        {
            "matchPackagePatterns": ["^NuGet.*"],
            "groupName": "all nuget dependencies",
            "groupSlug": "all-nuget",
            "labels": ["dependencies"]
        },
        {
            "matchPackagePatterns": ["^Octokit.*"],
            "groupName": "all octokit dependencies",
            "groupSlug": "all-octokit",
            "labels": ["dependencies"]
        },
        {
            "matchPackagePatterns": ["^PInvoke.*"],
            "groupName": "all pinvoke dependencies",
            "groupSlug": "all-pinvoke",
            "labels": ["dependencies"]
        },
        {
            "matchPackagePatterns": ["^Microsoft.Reactive.*", "^System.Reactive", "^System.Reactive.*"],
            "groupName": "all reactive extensions",
            "groupSlug": "all-rx",
            "labels": ["dependencies"]
        },
        {
            "matchPackagePatterns": ["^ReactiveUI.*"],
            "groupName": "all reactiveui dependencies",
            "groupSlug": "all-reactiveui",
            "labels": ["dependencies"]
        },
        {
            "matchPackagePatterns": ["^Whipstaff.*"],
            "groupName": "all whipstaff dependencies",
            "groupSlug": "all-whipstaff",
            "labels": ["dependencies"]
        },
        {
            "matchPackagePatterns": ["^Xamarin.*"],
            "groupName": "all xamarin dependencies",
            "groupSlug": "all-xamarin",
            "labels": ["dependencies"]
        },
        {
            "matchPackagePatterns": ["^xunit.*"],
            "groupName": "all xunit dependencies",
            "groupSlug": "all-xunit",
            "labels": ["dependencies"]
        }]
}
